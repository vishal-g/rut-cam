{
	"settings": {
		"appearanceTools": false,
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1280px"
		},
		"color": {
			"custom": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#F4F4F4",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#CC0033",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#5F6A72",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#FFFFFF",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#000000",
					"name": "Black",
					"slug": "black"
				}
			]
		},
		"typography": {
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Source Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/source-sans/SourceSansPro-Regular.otf"
							]
						},
						{
							"fontFamily": "Source Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/source-sans/SourceSansPro-Bold.otf"
							]
						}
					],
					"fontFamily": "\"Source Sans\", sans-serif",
					"name": "Source Sans",
					"slug": "source-sans"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"max": "1rem",
						"min": "1rem"
					},
					"size": "1rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.25rem",
						"min": "1.25rem"
					},
					"size": "1.25rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "1.5rem",
						"min": "1.5rem"
					},
					"size": "1.5rem",
					"slug": "large"
				},
				{
					"fluid": false,
					"size": "2.25rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "10rem",
						"min": "4rem"
					},
					"size": "10rem",
					"slug": "xx-large"
				}
			]
		},
		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"name": "1",
					"size": "clamp(1.5rem, 5vw, 2rem)",
					"slug": "30"
				},
				{
					"name": "2",
					"size": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)",
					"slug": "40"
				},
				{
					"name": "3",
					"size": "clamp(2.5rem, 8vw, 4.5rem)",
					"slug": "50"
				},
				{
					"name": "4",
					"size": "clamp(3.75rem, 10vw, 7rem)",
					"slug": "60"
				},
				{
					"name": "5",
					"size": "clamp(5rem, 5.25rem + ((1vw - 0.48rem) * 9.096), 8rem)",
					"slug": "70"
				},
				{
					"name": "6",
					"size": "clamp(7rem, 14vw, 11rem)",
					"slug": "80"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			}
		},
		"color": {
			"text": "var:preset|color|black"
		},
		"elements": {
			"button": {
				"color": {
					"background": "#CC0033",
					"text": "var:preset|color|white"
				}
			},
			"p": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "Source Sans",
					"fontSize": "3.625rem",
					"fontWeight": "700",
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "Source Sans",
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "700",
					"lineHeight": "1.2"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"spacing": {
					"margin": {
						"bottom": "25px",
						"left": "30px",
						"right": "30px",
						"top": "25px"
					}
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "Source Sans",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "700"
				}
			},
			"h4": {
				"color": {
					"text": "var(--wp--preset--color--secondary)"
				},
				"typography": {
					"fontFamily": "Source Sans",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "700"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "Source Sans",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "700",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "Source Sans",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"textTransform": "uppercase"
				}
			},
			"heading": {
				"color": {
					"text": "#000000"
				}
			},
			"link": {
				"color": {
					"text": "var:preset|color|black"

				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem",
			"padding": {
				"right": "var(--wp--preset--spacing--30)",
				"left": "var(--wp--preset--spacing--30)"
			}
		},
		"typography": {
			"fontFamily": "var:preset|font-family|source-sans",
			"fontSize": "var(--wp--preset--font-size--small)"
		}
	},
	"version": 2,
	"$schema": "https://schemas.wp.org/wp/6.1/theme.json"
}